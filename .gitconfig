[user]
    name = UNDEFINED
    email = UNDEFINED
[core]
    quotepath = false
    attributesfile = ~/.gitattributes
    excludesfile = /Users/tokuhy/.gitignore_global
[color]
    ui = auto
[init]
    templatedir = ~/.git_template
[difftool "sourcetree"]
    cmd = opendiff \"$LOCAL\" \"$REMOTE\"
    path =
[mergetool "sourcetree"]
    cmd = /Applications/SourceTree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
    trustExitCode = true
[alias]
    st = status -sb
    df = diff
    co = checkout
    b = branch
    pl = pull
    ps = push
